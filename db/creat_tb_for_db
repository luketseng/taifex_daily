CREATE TABLE II_Fut(
   Date TEXT   NOT NULL,
   Fut             TEXT    NOT NULL,
   Institutional   TEXT    NOT NULL,
   TR_B_Contract          INT,
   TR_B_Amount            INT,
   TR_S_Contract          INT,
   TR_S_Amount            INT,
   TR_Net_Contract        INT,
   TR_Net_Amount          INT,
   OI_B_Contract          INT,
   OI_B_Amount            INT,
   OI_S_Contract          INT,
   OI_S_Amount            INT,
   OI_Net_Contract        INT,
   OI_Net_Amount          INT
);

CREATE TABLE II_OP(
   Date TEXT   NOT NULL,
   OP              TEXT    NOT NULL,
   PC              TEXT    NOT NULL,
   Institutional   TEXT    NOT NULL,
   TR_B_Contract          INT,
   TR_B_Amount            INT,
   TR_S_Contract          INT,
   TR_S_Amount            INT,
   TR_Net_Contract        INT,
   TR_Net_Amount          INT,
   OI_B_Contract          INT,
   OI_B_Amount            INT,
   OI_S_Contract          INT,
   OI_S_Amount            INT,
   OI_Net_Contract        INT,
   OI_Net_Amount          INT
);

CREATE TABLE II_SPOT(
   Date TEXT   NOT NULL,
   Institutional   TEXT    NOT NULL,
   TR_B_Amount            INT,
   TR_S_Amount            INT,
   TR_Net_Amount          INT
);

